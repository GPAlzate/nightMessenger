{
    "name": "Night Mode Messenger",
    "version": "1.0",
    "description": "Makes a dark mode for Facebook Messenger",
    "manifest_version": 2,

    "offline_enabled": true,
    "content_scripts": [
        {
            "matches": ["https://www.messenger.com/*"],
            "run_at": "document_end",
            "css": ["styles.css"],
            "js": ["jquery-3.3.1.slim.min.js", "content.js"]
        }
    ]
}
